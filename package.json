{
  "name": "babel-plugin-extract-tags",
  "version": "0.5.0",
  "description": "Babel plugin to extracts tagged template literals to file. Files can be loaded by webpack.",
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "dependencies": {
    "mkdirp": "^0.5.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-register": "^6.24.0",
    "chokidar-cli": "^1.2.0",
    "rimraf": "^2.6.1",
    "tap-diff": "^0.1.1",
    "tape": "^4.6.3"
  },
  "scripts": {
    "clean": "rimraf lib",
    "build": "babel src -d lib",
    "pretest": "rimraf output test/**/*_tag*.*",
    "test": "tape -r babel-register test | tap-diff",
    "watch": "chokidar src test -i test/output -i test/**/*_tag*.* -c \"npm run test\"",
    "prepublish": "npm run clean && npm run build"
  },
  "author": "Harm Boschloo",
  "repository": {
    "type": "git",
    "url": "git://github.com/harmboschloo/babel-plugin-extract-tags.git"
  },
  "homepage": "https://github.com/harmboschloo/babel-plugin-extract-tags",
  "keywords": [
    "babel plugin",
    "tagged template literals",
    "webpack"
  ],
  "license": "MIT"
}
